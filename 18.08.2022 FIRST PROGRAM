#include<stdio.h>
int main()
{
    int n,i,j=0;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n-1;i++)
        scanf("%2d",&a[i]);
    for(i=0;i<n;i++)
    {
          j++;
          if(a[j]==(a[i]+1))
            continue;
          else
          {
            printf("%d",a[i]+1);
            break;
          }
    }
return 0;
}