
#include<stdio.h>
int EorD(int n);
int main()
{
    int n,result;
    printf("Enter a number:");
    scanf("%d",&n);
    result=EorD(n);
    if(result>0)
    printf("Even");
    else
    printf("Odd");
    return 0;
}
int EorD(int n)
{
    if(n%2==0)
    return 1;
    else
    return 0;
}